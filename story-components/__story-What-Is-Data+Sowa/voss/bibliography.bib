  intrahash = {8d465f3de7f4fb4291d32b2c9b4f43ae},
  journal = {Communications of the ACM},
  keywords = {OOP history patterns},
  month = {2},
  number = 2,
  pages = {123-128},
  timestamp = {2010-09-22T19:35:04.000+0200},
  title = {The Quarks of Object-Oriented Development},
  volume = 49,
  year = 2006
}

