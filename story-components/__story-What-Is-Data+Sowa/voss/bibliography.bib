  interhash = {ee8f0dfad692e25fd0023b1d178cb3a3},
  intrahash = {d4f67005a15fd993da298af562282dca},
  isbn = {978-90-481-2646-0},
  keywords = {phenomenology philosophy},
  pages = {35-55},
  publisher = {Springer},
  timestamp = {2011-07-04T10:47:04.000+0200},
  title = {A Toolbox of Phenomenological Methods},
