  keywords = {PEG language packrat parser programming},
  pages = {111-122},
  publisher = {ACM},
  timestamp = {2010-09-03T19:35:38.000+0200},
  title = {Parsing expression grammars: a recognition-based syntactic foundation.},
  url = {http://pdos.csail.mit.edu/~baford/packrat/popl04/},
